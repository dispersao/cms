{
  "connection": "default",
  "collectionName": "scripts",
  "info": {
    "name": "script",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "averagetime": {
      "type": "integer"
    },
    "author": {
      "model": "user",
      "via": "scripts",
      "plugin": "users-permissions"
    },
    "scriptsequences": {
      "collection": "scriptsequence",
      "via": "script"
    },
    "token": {
      "type": "string",
      "unique": true
    },
    "state": {
      "enum": [
        "idle",
        "started",
        "playing",
        "paused",
        "finished"
      ],
      "type": "enumeration",
      "required": true
    },
    "sessionpublications": {
      "collection": "sessionpublication",
      "via": "script"
    }
  }
}