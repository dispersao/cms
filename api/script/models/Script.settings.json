{
  "connection": "default",
  "collectionName": "scripts",
  "info": {
    "name": "script",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "averagetime": {
      "type": "integer"
    },
    "author": {
      "model": "user",
      "via": "scripts",
      "plugin": "users-permissions"
    },
    "speed": {
      "enum": [
        "normal",
        "ff",
        "sff"
      ],
      "type": "enumeration"
    },
    "scriptsequences": {
      "collection": "scriptsequence",
      "via": "script"
    }
  }
}